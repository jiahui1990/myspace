.processbar {
	position: relative;
	width: 755px;
	text-align: justify;
	margin: 0 auto;
	margin-top: 40px;
}
.processbar > span {
	position: relative;
	width: 64px;
	display: inline-block;
	text-align: center;
	background: #fff;
	z-index: 80;
	vertical-align: top;
}
.processbar > span > span {
	display: inline-block;
	width: 36px;
	height: 36px;
	border-radius: 50%;
	background: #20b6f0;
	font: normal 18px/36px "Arial";
	color: #fff;
	text-align: center;
	z-index: 20
}
.processbar > span > span.right {
	background: #20b6f0 url(../images/icon/right.png) no-repeat center center;
}
.processbar > span > div {
	margin-top: 8px;
	font: normal 14px "Microsoft YaHe","微软雅黑";
	text-align: center;
	position: absolute;
	width: 84px;
	white-space: nowrap;
	margin-left: -10px;
	color: #999;
}
