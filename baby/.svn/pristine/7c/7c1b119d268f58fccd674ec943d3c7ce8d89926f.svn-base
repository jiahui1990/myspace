.addenterprise {
	-webkit-box-sizing:border-box;
	-moz-box-sizing:border-box;
	box-sizing:border-box;
	display: inline-block;
	position: relative;
	margin-top: 18px;
	width: 514px;
	height: 241px;
	border: 3px  dashed #f3f3f3;
	text-align: center;
}
.ex2 {
	display: inline-block;
	width: 150px;
	height: 150px;
    position: relative;
    text-align: center;
    vertical-align: middle;
}
.ex2:after {
    position: absolute;
    -webkit-transform:rotate(45deg);
    -moz-transform:rotate(45deg);
    transform:rotate(45deg);
    top: 40px;
    bottom: 0;
    left: 5px;
    right: 0;
    content: "\274c";
    font-size: 120px; 
    color: #eee;
    line-height: 100px;
    text-align: center;
    vertical-align: middle;
}

div.ex2:after {
    content: '\d7';
}
