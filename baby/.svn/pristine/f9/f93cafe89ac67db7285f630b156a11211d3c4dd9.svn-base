package com.ggy.baby.userManage.pageModel;

import java.util.ArrayList;
import java.util.List;

import com.ggy.baby.monitorManage.model.Kindergarten;
import com.ggy.baby.monitorManage.model.KindergartenArea;
import com.ggy.baby.userManage.model.ViewUserArea;

import javacommon.base.pageModel.JumpInfo;

/**
 * 用户管理Out参数
 * @author Chencong
 *
 */
public class UserManageOut {
	/**
	 * 页面跳转对象
	 */
	private JumpInfo jumpInfo = new JumpInfo();
	/**
	 * 用户区域视图集合
	 */
	private List<ViewUserArea> viewUserAreaList = new ArrayList<ViewUserArea>();
	/**
	 * 幼儿园集合
	 */
	private List<Kindergarten> kindergartenList = new ArrayList<Kindergarten>();
	/**
	 * 区域集合
	 */
	private List<KindergartenArea> kindergartenAreaList = new ArrayList<KindergartenArea>();
	/**
	 * 页数
	 */
	private Integer pages = 0;

	public JumpInfo getJumpInfo() {
		return jumpInfo;
	}

	public void setJumpInfo(JumpInfo jumpInfo) {
		this.jumpInfo = jumpInfo;
	}

	public List<Kindergarten> getKindergartenList() {
		return kindergartenList;
	}

	public void setKindergartenList(List<Kindergarten> kindergartenList) {
		this.kindergartenList = kindergartenList;
	}

	public List<KindergartenArea> getKindergartenAreaList() {
		return kindergartenAreaList;
	}

	public void setKindergartenAreaList(List<KindergartenArea> kindergartenAreaList) {
		this.kindergartenAreaList = kindergartenAreaList;
	}

	public List<ViewUserArea> getViewUserAreaList() {
		return viewUserAreaList;
	}

	public void setViewUserAreaList(List<ViewUserArea> viewUserAreaList) {
		this.viewUserAreaList = viewUserAreaList;
	}

	public Integer getPages() {
		return pages;
	}

	public void setPages(Integer pages) {
		this.pages = pages;
	}
}
