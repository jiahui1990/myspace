.inner-line{
	border: 1px solid #DDD;
	position: relative;
}
/*留下空白*/
.p{
	padding: 45px;
}
/*button*/
.btn{
	display: block;
	background: #3babd0;
	width: 120px;
	height: 44px;
	text-align: center;
}
.btn:active,.btnsubmit:active{
	background: #218bae;
}
/*submit提交按钮*/
.btnsubmit{
	display: block;;
	background: #3babd0;
	width: 149px;
	height: 44px;
	text-align: center;;
	line-height: 44px;
}
/*c1*/
.c1{
	color: #3BABD0;
	background: url() no-repeat;
}
/*input*/
.userinput{
	background: none repeat scroll 0 0 #E0E0E0;
    border: 0 none;
    height: 44px;
    margin-left: 11px;
    padding-left: 5px;
    width: 257px;
    line-height: 14px;
}
#username,#userpic,#basicinfo,#allow,#scuQuestion{
	margin-top: 18px;

}
.w75{
	display: inline-block;
	width: 75px;
	height: 100%;
	text-align: right;
}
.upload{
	line-height: 44px;
}
.tips{
	display: inline-block;
	margin-left: 8px;
}
#submitCeInfo{
	margin: 0 auto;
}
.warn{
	background: url(../../images/icon/warning.png) no-repeat;
	padding-left: 40px;
	vertical-align: middle;
	line-height: 32px;
}