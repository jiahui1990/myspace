/*=================header CSS===================*/
.header {
    background: none repeat scroll 0 0 #333;
    height: 64px;
    position: fixed;
    width: 100%;
    z-index: 20;
}
.header-inner {
    height: inherit;
    margin: 0 auto;
    position: relative;
    width: 1100px;
}
.logo {
    background:url(../images/RNG/logo.png) left center no-repeat;
    height: 64px;
    width: 200px;
	display:inline-block;
	text-indent:-9999px;
	}
.search {
    display: inline-block;
    height: 35px;
    margin-top: 14px;
    position: relative;
    width: 396px;
}
.input-search {
    background: none repeat scroll 0 0 #333;
    border: 3px solid #5852F4;
    color: #7D7C7C;
    height: 29px;
    width: 390px;
}
.btn-search {
    background: none repeat scroll 0 0 #5852F4;
    color: #FFFFFF;
    font-family: "微软雅黑";
    font-size: 16px;
    height: 35px;
    line-height: 35px;
    position: absolute;
    right: 0;
    top: 0;
    width: 74px;
}
.icon-search {
    background:url(../images/RNG/search.png) repeat scroll 0 0 rgba(0, 0, 0, 0);
    display: inline-block;
    height: 20px;
    margin-right: 3px;
    position: relative;
    top: 4px;
    width: 20px;
}
/*=================nav CSS===================*/
.nav{
	display: inline-block;
	font-family: "宋体";
	font-size:14px;
	color: #FFF;
}
.nav a{
	margin-right: 20px;
	color: #FFF;
	height:64px;
	line-height:64px;
}
/*=================content CSS===================*/
.content {
    margin: 0 auto;
    padding-top: 82px;
    width: 1100px;
}
/*=================info CSS===================*/
.info{
	background: #FFF;
	padding: 18px;
}
.modifyhead{
	display: inline-block;
	margin: 15px;
	width: 120px;
}
.avatar{
	width: 119px;
	height: 119px;
	overflow: hidden;
	background:url(../images/RNG/head-portrait.png) no-repeat;
	margin-bottom: 8px;
}
.accountinfo{
	line-height: 30px;
	display: inline-block;
	position: absolute;
	top: 40px;
	left: 150px;
}
.certification{
	position: absolute;
	top: 40px;
	right: 250px;
}
.certification span{
	display: inline-block;
	width: 84px;
	padding-top: 85px;
	text-align: center;
	margin-right: 20px;
}
.icon-realname{
	background:url(../images/RNG/icon1.png) no-repeat!important;
	}
.icon-clientsetup{
	background: url(../images/RNG/icon2.png) no-repeat!important;
}
.icon-mobilebound{
	background: url(../images/RNG/icon3.png) no-repeat!important;
}
.icon-mailboxbinding{
	background: url(../images/RNG/icon4.png) no-repeat!important;
}
.changepassword{
	position: absolute;
	right: 18px;
	top: 70px;
}
