package com.ggy.baby.monitorManage.model;

import javacommon.base.model.GlobalVariable;

/**
 * 监控对象
 * @author Chencong
 *
 */
public class Monitor {
	/**
	 * 监控id
	 */
	private Long id;
	/**
	 * 监控所属区域id
	 */
	private KindergartenArea areaId;
	/**
	 * 监控名称
	 */
	private String name;
	/**
	 * 监控sn号
	 */
	private String sn;
	/**
	 * 开始观看时间
	 */
	private String beginWatchTime;
	/**
	 * 结束观看时间
	 */
	private String endWatchTime;
	/**
	 * 创建时间
	 */
	private String createTime;
	/**
	 * 是否被保存了
	 */
	private GlobalVariable isSaved;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public KindergartenArea getAreaId() {
		return areaId;
	}

	public void setAreaId(KindergartenArea areaId) {
		this.areaId = areaId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getSn() {
		return sn;
	}

	public void setSn(String sn) {
		this.sn = sn;
	}

	public String getBeginWatchTime() {
		return beginWatchTime;
	}

	public void setBeginWatchTime(String beginWatchTime) {
		this.beginWatchTime = beginWatchTime;
	}

	public String getEndWatchTime() {
		return endWatchTime;
	}

	public void setEndWatchTime(String endWatchTime) {
		this.endWatchTime = endWatchTime;
	}

	public String getCreateTime() {
		return createTime;
	}

	public void setCreateTime(String createTime) {
		this.createTime = createTime;
	}

	public Monitor() {
		super();
	}

	public Monitor(Long id, KindergartenArea areaId, String name, String sn,
			String beginWatchTime, String endWatchTime, String createTime) {
		super();
		this.id = id;
		this.areaId = areaId;
		this.name = name;
		this.sn = sn;
		this.beginWatchTime = beginWatchTime;
		this.endWatchTime = endWatchTime;
		this.createTime = createTime;
	}

	public GlobalVariable getIsSaved() {
		return isSaved;
	}

	public void setIsSaved(GlobalVariable isSaved) {
		this.isSaved = isSaved;
	}
}
