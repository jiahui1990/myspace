	         	</tr>
	         	
	         	<c:forEach items="${viewUserAreas }" var="ua" >
	         		<tr height="28px;" 
	         		data-id="${ua.userId }" 
	         		data-username="${ua.userName }"
	         		data-roleId="${ua.roleId }" 
	         		data-roleName="${ua.roleName }"
	         		data-kinId="${ua.kindergartenId }" 
	         		data-pkaId="${ua.pkaId }" 
	         		data-kaId="${ua.areaId }"
	         		data-starttime="${ua.operationTime }"
	         		data-endtime = "${ua.expireTime }">
	         		<td width="15px"><input type="checkbox" class="check_id" value="${ua.userId }" /></td>
	         		<td width="150px;"  align="center">${ua.userName }</td>
	         		<td width="150px;" align="center">${ua.kindergartenName }</td>
	         		<td width="150px;" align="center">${ua.areaName }</td>
	         		<td width="120px;" align="center">${ua.telphone }</td>
	         		<td width="150px;" align="center">${fn:substring(ua.operationTime,0,10) }</td>
	         		<td width="150px;" align="center">${fn:substring(ua.expireTime,0,10) }</td>
	         		<td width="180px;" align="center">${ua.roleName }</td>
	         		<td width="150px;" align="center"><a class="delete" href="javascript:void();" style="color: blue;margin: 0px 5px;">删除</a><a class="update" href="javascript:void();" style="color: blue;margin: 0px 5px;">编辑</a></td>
