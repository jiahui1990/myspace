    	<form id="form_update">
    		<input type="hidden" name="In.users.id" id="userId" />
    		<input type="hidden" name="In.roleId" id="roleId" />
    		<input type="hidden" id="kinderId" />
    		<input type="hidden" id="pkaId" />
    		<input type="hidden" id="kaId" />
    		<table style="border-spacing:8px 15px; text-align: center;font-size: 18px;font-family: 'Arial Normal', 'Arial'; font-weight: 400; font-style: normal; color: #333333;background: #fff;">
    			<thead><tr><td colspan="2"> 修改用户</td></tr></thead>
    			<tr>
    				<td >权限</td>
    				<td><span id="roleName"></span></td>
    			</tr>
    			<tr>
    				<td >用户名</td>
    				<td><input id="username" type="text" name="In.users.username" style="width: 220px;height: 29px;text-align: center;" /></td>
    			</tr>
    			<tr>
    				<td >密码</td>
    				<td><input id="password" type="password" name="In.users.password" style="width: 220px;height: 29px;text-align: center;" /></td>
    			</tr>
    			<tr>
    				<td >确认密码</td>
    				<td><input id="password_confirm" type="password" style="width: 220px;height: 29px;text-align: center;" /></td>
    			</tr>
    			<tr >
           			<td width="81px;" style="font-family: 'Arial Normal', 'Arial'; font-weight: 400; font-style: normal; color: #333333;">生效时间</td>
           			<td width="220px;"><input id="starttime" name="In.users.operationTime" style="width: 220px;height: 29px;text-align: center;"/></td>
           		</tr>
           		<tr id="tr_ex">
           			<td width="81px;" style="font-family: 'Arial Normal', 'Arial'; font-weight: 400; font-style: normal; color: #333333;">过期时间</td>
           			<td width="220px;"><input id="endtime" name="In.users.expireTime" style="width: 220px;height: 29px;text-align: center;"/></td>
           		</tr>
    			<tr id="tr_kin">
    				<td>幼儿园选择</td>
    				<td><select id="kinId" name="In.kindergarten.id" style="width: 220px;height: 29px;text-align: center;">
							<c:forEach items="${kinders}" var="kin">
								<option value="${kin.id}">${kin.name }</option>
							</c:forEach>
						</select>
					</td>
    			</tr>
    			<tr id="tr_area">
    				<td>区域选择</td>
    				<td>
    					<select id="areaId" style="width: 220px;height: 29px;text-align: center;"></select>
    				</td>
    			</tr>
    			<tr id="tr_carea">
          			<td width="90px;" style="font-family: 'Arial Normal', 'Arial'; font-weight: 400; font-style: normal; color: #333333;">分区名</td>
          			<td width="220px;">
          				<select id="careaId" style="width: 220px;height: 29px;text-align: center;" name="In.area.id">
          					
          				</select>
          		</tr>
          		<tr id="tr_btn">
          			<td colspan="2">
