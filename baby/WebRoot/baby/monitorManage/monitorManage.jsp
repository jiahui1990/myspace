           				<select id="kinId" style="width: 220px;height: 29px;text-align: center;">
           					<c:forEach items="${kinders}" var="kin">
           						<option value="${kin.id}">${kin.name }</option>
           					</c:forEach>
           				</select>
           			</td>
           		</tr>
